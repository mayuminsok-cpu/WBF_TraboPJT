{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "lib": [
            "ES2020"
        ],
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": true,
        "types": [
            "node"
        ]
    },
    "include": [
        "*.ts",
        "*.d.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}